					},
					"methods":{
						"type":"array",
						"items":{
							"type":"string"
						}
					}
				}		
			}			
        },        
		"rangeList":{            
            "type":"array",
			"items":{
				"type":"object",
				"properties":{					
					"features":{
						"type":"string"
					},
					"start":{
