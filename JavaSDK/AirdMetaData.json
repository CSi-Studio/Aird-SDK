		"type":{            
            "type":"string"
        },        
		"airdPath":{            
            "type":"string"
        },        
		"fileSize":{            
            "type":"integer"
        },        
		"totalScanCount":{            
            "type":"integer"
        },        
		"creator":{            
            "type":"string"
        },        
		"createDate":{            
            "type":"string"
        },        
		"features":{            
            "type":"string"
        },
		"version":{            
            "type":"string"
        }, 
		"versionCode":{            
            "type":"integer"
        },         
		"ignoreZeroIntensityPoint":{            
            "type":"boolean"
        }
    }
