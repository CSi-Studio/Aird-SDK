{
	"$schema":"http://json-schema.org/draft-04/schema#",
    "title":"metaData",
	"description":"mata data of 'Aird' V1",
    "type":"object",
    "properties":{
        "instruments":{            
            "type":"array",
			"items":{
				"type":"array",
				"items":{
					"type":"object",
					"properties":{
						"manufacturer":{
							"type":"string"
						},
						"model":{
							"type":"string"
						},
						"source":{
							"type":"array",
							"items":{
								"type":"string"
							}
						},
						"analyzer":{
							"type":"array",
							"items":{
								"type":"string"
							}
						},
						"detector":{
							"type":"array",
							"items":{
								"type":"string"
							}
						}
					}
				}
			}
        },        
		"softwares":{            
            "type":"array",
			"items":{
				"type":"object",
				"properties":{
					"name":{
						"type":"string"
					},
					"type":{
						"type":"string"
					},
					"version":{
						"type":"string"
					}
				}
			}
        },        
		"parentFiles":{            
            "type":"array",
			"items":{
				"type":"object",
				"properties":{
					"name":{
						"type":"string"
					},
					"location":{
						"type":"string"
					},
					"formatType":{
						"type":"string"
					}
				}			
			}
        },        
		"compressors":{            
            "type":"array",
			"items":{
				"type":"object",
				"properties":{
					"precision":{
						"type":"integer"
					},
					"target":{
