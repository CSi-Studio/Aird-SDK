					"nums":{
						"type":"array",
						"items":{
							"type":"integer"
						}
					},
					"rts":{
						"type":"array",
						"items":{
							"type":"number"
						}
					},
					"mzs":{
						"type":"array",
						"items":{
							"type":"integer"
						}
					},
					"ints":{
						"type":"array",
						"items":{
							"type":"integer"
						}
					}
				}			
